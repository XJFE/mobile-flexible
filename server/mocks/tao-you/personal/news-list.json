/**
 * [淘游助手] 新闻列表数据接口
 * 首页中使用、需要分页
 *
 * @url /tao_you/news_list
 *
 * 参数说明：
 *
 * result: 新闻列表
 * id: 新闻ID
 * type: 类型（用来标识显示的方式）
 * title: 标题
 * picPath: 缩略图
 * source: 来源
 * link: 链接地址
 * publishTime: 发布时间
 */

{
  "code|1": [10000] ,
  "result|8": [
    {
      "id|+1": 1,
      "type|1": [1, 2, 3, 4],
      "title|3": "@ctitle",
      "picPath": ["@image(120x90)"],
      "desc": "@cparagraph",
      "picPathList": ["@image(120x120)","@image(120x120)","@image(120x120)"],
      "source": "@ctitle",
      "link": "/news/@id",
      "publishTime|1": ["刚刚", "2分钟前", "4分钟前"]
    }
  ]
}
