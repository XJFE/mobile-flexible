/**
 * [淘游助手] 我的游戏列表数据接口
 *
 * @url /tao_you/my_game_list
 *
 * 参数说明：
 *
 * result: 游戏列表
 * id: 游戏ID
 * name: 游戏名
 * picPath: 缩略图
 * linkUrl: 链接地址
 * publishTime: 发布时间
 */

{
  "code|1": [10000] ,
  "result|8": [
    {
      "id|+1": 1,
      "field|1": ["1", "2"],
      "name": "@ctitle",
      "imgUrl": "@image(120x120)",
      "link": "/game-intro/@id",
      "publishTime": "@date"
    }
  ]
}
