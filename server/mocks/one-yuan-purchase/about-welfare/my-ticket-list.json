/**
 * [一元购] 抵扣券列表接口
 * 我的淘豆页面中使用
 *
 * @url /my_ticket_list
 *
 * 参数说明：
 *
 * list: 抵扣券列表
 * id: 抵扣券ID
 * name: 券名字
 * price: 券的面额
 * startTime: 券的使用开始时间
 * endTime: 券的使用戒指时间
 * status: 券的状态（0:有效、1:已使用、3:已过期）
 * remark: 券的说明
 */

{
  "code|1": [10000] ,
  "info":{"taoDou": "107882256"},
  "list|0-3": [
    {
      "id|+1": 1,
      "name": "抵扣券",
      "prize|1": [50, 80, 100, 200, 500],
      "startTime": "@date('yyyy-mm-dd HH:mm')",
      "endTime": "@date('yyyy-mm-dd HH:mm')",
      "status|1": [0, 1, 3],
      "remark": "@ctitle"
    }
  ]
}
